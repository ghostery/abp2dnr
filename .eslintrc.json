{
  "extends": "eslint-config-eyeo",
  "root": true,
  "env": {
    "node": true
  },
  "rules": {
    "no-undefined": "off",
    "no-warning-comments": "off",
    "space-before-function-paren": ["error", {
      "anonymous": "never",
      "named": "never",
      "asyncArrow": "always"
    }]
  }
}
